}
if (marquee02.length > 0) {
    gsap.set(marquee02,{y:50})
}


function marqueeFromLeft(box, time){
  // Verificar que existan elementos antes de animar
  if (!box || box.length === 0) return;
  
  //initially position each box in a row
  gsap.set(box, {
    x:function(i) {
      return i * boxWidth;
    },
    force3D: true, rotation: 0.01, y: 0
  });
  
  var action = gsap.timeline()
  .to(box, {
    duration:time,
    ease: 'none',
    x: "+=" + modulusValue, //move each box modulusValue to RIGHT
    modifiers: {
      x: function(x) {
        x = parseInt(x);
        var newX = x % modulusValue; 
        return newX + 'px';
      }
    },
    repeat:-1,
  });
  return action
}

function marqueeFromRight(box, time){
  // Verificar que existan elementos antes de animar
  if (!box || box.length === 0) return;
  
  gsap.set(box, {
    x:function(i) {
      return i * boxWidth;
    },
    force3D: true, rotation: 0.01, y: 0
  });
  
  var action = gsap.timeline()
  .to(box, {
    duration:time,
    ease: 'none',
    x: "-=" + modulusValue, //move each box modulusValue to left
    modifiers: {
      x: function(x) {
        x = parseInt(x);
        //https://greensock.com/forums/topic/19874-change-direction-of-carousel-wrap-example/
        var newX = x<0 ? (x % modulusValue) + modulusValue : x;
        return newX + 'px';
      }
    },
    repeat:-1,
  });
  return action
}


// ===============================
// Solo crear timeline si hay elementos para animar
if (marquee01Box.length > 0 || marquee02Box.length > 0) {
    var master = gsap.timeline()
    .add(marqueeFromLeft(marquee01Box, 60))
    .add(marqueeFromRight(marquee02Box, 40), 1)
}

