<script>
// Script para ocultar Social-rrss del footer cuando Contact esté en viewport
if (typeof window !== 'undefined') {
    const hideFooterSocialRrss = () => {
        const footerSocialRrss = document.querySelector('footer .social-rrss, footer [class*="social"]') as HTMLElement;
        if (footerSocialRrss) {
            footerSocialRrss.style.opacity = '0';
            footerSocialRrss.style.visibility = 'hidden';
            footerSocialRrss.style.transition = 'opacity 0.3s ease, visibility 0.3s ease';
        }
    };

    const showFooterSocialRrss = () => {
        const footerSocialRrss = document.querySelector('footer .social-rrss, footer [class*="social"]') as HTMLElement;
        if (footerSocialRrss) {
            footerSocialRrss.style.opacity = '1';
            footerSocialRrss.style.visibility = 'visible';
            footerSocialRrss.style.transition = 'opacity 0.3s ease, visibility 0.3s ease';
        }
    };

    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                // Contact está visible, ocultar Social-rrss del footer
                hideFooterSocialRrss();
            } else {
                // Contact no está visible, mostrar Social-rrss del footer
                showFooterSocialRrss();
            }
        });
    }, {
        threshold: 0.5,  // Aumentar threshold para mayor precisión
        rootMargin: '0px 0px -10% 0px'  // Margen para activar solo cuando esté realmente visible
    });

    // Observar la sección Contact
    const contactSection = document.querySelector('#contact');
    if (contactSection) {
        observer.observe(contactSection);
    }

    // También verificar al cargar la página
    setTimeout(() => {
        const contactSection = document.querySelector('#contact');
        if (contactSection) {
            const rect = contactSection.getBoundingClientRect();
            const isVisible = rect.top < window.innerHeight && rect.bottom > 0;
            
            if (isVisible) {
                hideFooterSocialRrss();
            } else {
                showFooterSocialRrss();
            }
        }
    }, 100);
}
</script>

